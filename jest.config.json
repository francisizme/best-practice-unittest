{
  "transform":{
    "^.+\\.tsx?$":"ts-jest"
  },
  "setupFilesAfterEnv":[
    "./jestMockDependencies.js"
  ],
  "coverageReporters":[
    "lcov",
    "text",
    "text-summary"
  ],
  "collectCoverageFrom":[
    "<rootDir>/src/**/*.ts",
    "!**/node_modules/**"
  ],
  "testRegex":"(\\.(test|spec))\\.(ts|tsx|js)$",
  "moduleFileExtensions":[
    "ts",
    "tsx",
    "js",
    "json"
  ],
  "testEnvironment":"node",
  "verbose":true
}
